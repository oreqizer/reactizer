{
  "name": "@reactizer/boil-base",
  "version": "0.29.8",
  "description": "The ultimate React boilerplate!",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/_server/index.js",
  "scripts": {
    "start": "rimraf dist && yarn statics && run-p devserver webpack:dev",
    "devserver": "run-p statics babel && run-p run:dev babel:dev",
    "run": "node dist/_server",
    "run:dev": "nodemon dist/_server",
    "render": "node dist/_server/render",
    "bundle": "rimraf dist && run-p statics babel webpack && yarn render",
    "babel": "babel src -d dist --extensions '.ts,.tsx' --ignore '**/__tests__/*'",
    "babel:dev": "yarn babel --watch --source-maps inline",
    "webpack": "webpack --env production",
    "webpack:dev": "webpack-dev-server --colors",
    "webpack:stats": "env NODE_ENV=production webpack --env production --json > stats.json",
    "keys": "node etc/bin/keys",
    "statics": "copyfiles 'public/**/*' -u 1 dist/static && node etc/bin/statics",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "types": "tsc",
    "ci": "run-p test:coverage lint types madge",
    "lint": "eslint './**/*.{js,ts,tsx,jsx}'",
    "madge": "madge -c src/_server/index.ts src/app/index.ts",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -o dist/static/storybook -s public",
    "heroku-postbuild": "SENTRY_RELEASE=$SOURCE_VERSION yarn bundle"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn ci"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@reactizer/intl": "^0.28.0",
    "@reactizer/theme": "^0.27.1",
    "@sentry/browser": "^5.11.0",
    "@sentry/node": "^5.11.0",
    "dotenv-safe": "^8.2.0",
    "fs-extra": "^8.1.0",
    "history": "^4.10.1",
    "koa": "^2.10.0",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^5.2.0",
    "koa-logger": "^3.2.1",
    "koa-sslify": "^4.0.3",
    "koa-static": "^5.0.0",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "regenerator-runtime": "^0.13.3",
    "rhythm-fns": "^1.0.0",
    "styled-components": "^5.0.0-rc.2",
    "styled-normalize": "^8.0.6",
    "wouter": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@loadable/babel-plugin": "^5.12.0",
    "@reactizer/babel-preset": "^0.28.0",
    "@reactizer/cli": "^0.29.0",
    "@reactizer/eslint-config": "^0.28.0",
    "@reactizer/webpack": "^0.31.0",
    "@storybook/addon-actions": "5.3.1",
    "@storybook/addon-knobs": "^5.3.1",
    "@storybook/addon-links": "5.3.1",
    "@storybook/addons": "5.3.1",
    "@storybook/react": "5.3.1",
    "@types/dotenv-safe": "^8.1.0",
    "@types/enzyme": "^3.10.4",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.25",
    "@types/koa": "^2.11.0",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-sslify": "^4.0.1",
    "@types/koa-static": "^4.0.1",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.9.1",
    "@types/node": "^13.1.6",
    "@types/node-polyglot": "^0.4.34",
    "@types/ramda": "^0.26.39",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.2",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^4.4.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "commitlint": "^8.3.4",
    "copyfiles": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "husky": "^4.0.7",
    "i18n-extract": "^0.6.6",
    "jest": "^24.9.0",
    "madge": "^3.4.4",
    "md5": "^2.2.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "raf": "^3.4.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "gitHead": "72a4ac5c1b5c307ec284ddc7ee6380eb66f5de6d"
}
