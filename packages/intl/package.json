{
  "name": "@reactizer/intl",
  "version": "0.24.3",
  "description": "Reactizer's intl.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn typegen && yarn babel",
    "typegen": "run-p typegen:* && rimraf '{es,lib}/**/*.{js,jsx}'",
    "typegen:es": "tsc --outDir es",
    "typegen:lib": "tsc --outDir lib",
    "babel": "run-p babel:*",
    "babel:cjs": "babel src -d lib --extensions '.ts,.tsx' --ignore '**/__tests__/*'",
    "babel:es": "cross-env NODE_ENV=modules babel src -d es --extensions '.ts,.tsx' --ignore '**/__tests__/*'",
    "types": "tsc --noEmit"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "node-polyglot": "^2.4.0"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@types/node-polyglot": "^0.4.34"
  },
  "license": "MIT",
  "gitHead": "e1aef0ca035beaf994a7c446ee220a414df24792"
}
