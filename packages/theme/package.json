{
  "name": "@reactizer/theme",
  "version": "0.24.5",
  "description": "Reactizer's theme.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn typegen && yarn babel",
    "typegen": "run-p typegen:* && rimraf '{es,lib}/**/*.{js,jsx}'",
    "typegen:es": "tsc --outDir es",
    "typegen:lib": "tsc --outDir lib",
    "babel": "run-p babel:*",
    "babel:cjs": "babel src -d lib --extensions '.ts,.tsx' --ignore '**/__tests__/*'",
    "babel:es": "cross-env NODE_ENV=modules babel src -d es --extensions '.ts,.tsx' --ignore '**/__tests__/*'",
    "types": "tsc --noEmit"
  },
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "dependencies": {
    "rhythm-fns": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "styled-components": "^5.0.0-rc.2"
  },
  "license": "MIT",
  "gitHead": "e112cf5290897641088833ed937f766f7085d1b5"
}
