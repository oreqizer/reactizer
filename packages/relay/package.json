{
  "name": "@reactizer/relay",
  "version": "0.12.6",
  "description": "The ultimate React boilerplate!",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/_server/index.js",
  "scripts": {
    "start": "rimraf dist && yarn statics && run-p devserver webpack:dev",
    "devserver": "run-p statics babel && run-p run:dev babel:dev",
    "run": "node dist/_server",
    "run:dev": "nodemon dist/_server",
    "render": "node dist/_server/render",
    "bundle": "rimraf dist && run-p statics babel webpack && yarn render",
    "babel": "babel src -d dist --extensions '.ts,.tsx' --ignore '**/__tests__/*'",
    "babel:dev": "yarn babel --watch --source-maps inline",
    "webpack": "webpack --config etc/webpack.build.js --mode production",
    "webpack:dev": "webpack-dev-server --config etc/webpack.dev.js --mode development --inline --hot --progress --colors",
    "webpack:stats": "env NODE_ENV=production yarn webpack -- --json > stats.json",
    "syncgql": "run-s schema generate relay",
    "relay": "relay-compiler --src src --schema ./schema.graphql --extensions ts tsx --artifactDirectory src/__generated__ --language typescript",
    "relay:validate": "yarn relay --validate",
    "schema": "graphql get-schema",
    "generate": "graphql-codegen",
    "keys": "node etc/bin/keys",
    "statics": "copyfiles 'public/**/*' -u 1 dist/static && node etc/bin/statics",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "types": "tsc",
    "ci": "run-p test:coverage lint types madge",
    "lint": "eslint './**/*.{js,ts,tsx,jsx}'",
    "madge": "madge -c src/_server/index.ts src/app/index.ts",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -o dist/static/storybook -s public",
    "heroku-postbuild": "SENTRY_RELEASE=$SOURCE_VERSION yarn bundle"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn ci"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        "sentence-case"
      ]
    }
  },
  "resolutions": {
    "@types/react": "^16.9.9",
    "react-helmet/react-side-effect": "2.1.0"
  },
  "dependencies": {
    "@loadable/component": "^5.10.3",
